let Used_Artificial_Intelligence = false;
let Usei_Inteligencia_Artificial = false;
let Used_Previous_Template = true;
let Usei_Template_Previo = true;

if (Used_Artificial_Intelligence === true) {
    var Approximate_Level_Used = "0%";
    var How_Used = "";
}
if (Used_Previous_Template === true) {
    var Credit = "Credits for the previous templates go to an unknown author from CodePen (Unfortunately, the author could not be found).";
} else {
    var Credit = "100% original";
}

if (Usei_Inteligencia_Artificial === true) {
    var Nivel_Aproximado_Usado = "0%";
    var Como_Usei = "";
}
if (Usei_Template_Previo === true) {
    var Credito = "Os créditos dos templates anteriores vão para um autor desconhecido do CodePen (Infelizmente, o autor não pôde ser encontrado).";
} else {
    var Credito = "100% autoral";
}

console.log("English Documentation:");
console.log("Approximate Level Used:", Approximate_Level_Used);
console.log("How Used:", How_Used);
console.log("Credit:", Credit);

console.log("\nPortuguese Documentation:");
console.log("Nível Aproximado Usado:", Nivel_Aproximado_Usado);
console.log("Como Usei:", Como_Usei);
console.log("Crédito:", Credito);
